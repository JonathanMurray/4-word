<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FixedAspectRatioFrameLayout">
        <attr name="aspectRatioWidth" format="integer" />
        <attr name="aspectRatioHeight" format="integer" />
    </declare-styleable>

    <declare-styleable name="NumberPickerView">
        <attr name="title" format="string" />
        <attr name="value" format="integer"/>
        <attr name="minValue" format="integer"/>
        <attr name="maxValue" format="integer"/>

    </declare-styleable>

    <declare-styleable name="AvatarView">
        <attr name="playerName" format="string" />
        <attr name="unknownAvatar" format="boolean"/>
        <attr name="highlighted" format="boolean"/>
        <attr name="nameVisible" format="boolean"/>
    </declare-styleable>


</resources>